<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_info"
    app:startDestination="@id/viewInfoFragment_info">

    <activity
        android:id="@+id/infoActivity_info"
        android:name="com.example.assignment2.info.InfoActivity"
        android:label="activity_info"
        tools:layout="@layout/activity_info" />
    <fragment
        android:id="@+id/journalFragment_info"
        android:name="com.example.assignment2.JournalFragment"
        android:label="fragment_journal"
        tools:layout="@layout/fragment_journal" >
        <action
            android:id="@+id/action_journalFragment_info_to_viewInfoFragment"
            app:destination="@id/viewInfoFragment_info" />
    </fragment>
    <fragment
        android:id="@+id/viewInfoFragment_info"
        android:name="com.example.assignment2.info.ViewInfoFragment"
        android:label="fragment_info"
        tools:layout="@layout/fragment_view_info" >
        <action
            android:id="@+id/action_viewInfoFragment_to_journalFragment_info"
            app:destination="@id/journalFragment_info" />
        <action
            android:id="@+id/action_viewInfoFragment_info_to_dietFragment2"
            app:destination="@id/dietFragment2" />
        <action
            android:id="@+id/action_viewInfoFragment_info_to_measurementFragment3"
            app:destination="@id/measurementFragment3" />
        <action
            android:id="@+id/action_viewInfoFragment_info_to_sleepFragment2"
            app:destination="@id/sleepFragment2" />
    </fragment>
    <fragment
        android:id="@+id/infoFragment2"
        android:name="com.example.assignment2.info.InfoFragment"
        android:label="fragment_info"
        tools:layout="@layout/fragment_info" />
    <fragment
        android:id="@+id/infoExpertFragment"
        android:name="com.example.assignment2.info.InfoExpertFragment"
        android:label="fragment_info_expert"
        tools:layout="@layout/fragment_info_expert" />
    <fragment
        android:id="@+id/infoVideoFragment"
        android:name="com.example.assignment2.info.InfoVideoFragment"
        android:label="fragment_info_video"
        tools:layout="@layout/fragment_info_video" />
    <fragment
        android:id="@+id/dietFragment2"
        android:name="com.example.assignment2.DietFragment"
        android:label="fragment_diet"
        tools:layout="@layout/fragment_diet" />
    <fragment
        android:id="@+id/measurementFragment3"
        android:name="com.example.assignment2.measurement.MeasurementFragment"
        android:label="fragment_measurement"
        tools:layout="@layout/fragment_measurement" >
        <action
            android:id="@+id/measureFragment2"
            app:destination="@id/viewInfoFragment_info" />
    </fragment>
    <fragment
        android:id="@+id/sleepFragment2"
        android:name="com.example.assignment2.SleepFragment"
        android:label="fragment_sleep"
        tools:layout="@layout/fragment_sleep" >
        <action
            android:id="@+id/action_sleepFragment2_to_viewInfoFragment_info"
            app:destination="@id/viewInfoFragment_info" />
    </fragment>
    <activity
        android:id="@+id/logoutActivity2"
        android:name="com.example.assignment2.LogoutActivity"
        android:label="activity_logout"
        tools:layout="@layout/activity_logout" />
</navigation>