        android:id="@+id/heightFragment_btm"
        android:name="com.example.assignment2.HeightFragment"
        android:label="fragment_height"
        tools:layout="@layout/fragment_height" />
    <fragment
        android:id="@+id/weightFragment_btm"
        android:name="com.example.assignment2.WeightFragment"
        android:label="fragment_weight"
        tools:layout="@layout/fragment_weight" />
    <fragment
        android:id="@+id/checkinFragment_btm"
        android:name="com.example.assignment2.CheckinFragment"
        android:label="fragment_checkin"
        tools:layout="@layout/fragment_checkin" />
</navigation>